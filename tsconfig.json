{
  "compilerOptions": {
      "rootDir": "..",
      "target": "es5",
      "lib": ["ESNext"],
      "moduleResolution": "node",
      "removeComments": false,
      "sourceMap": true,
      "declaration": true,
      "strict": true,
      "module": "ESNext",
      "noEmitOnError": true,
      "esModuleInterop": true,
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}